    <script>
      //  iframe中接受  src/components/pageModal/PageModal.tsx 中的 方法直接新开页面
      window.addEventListener('message', event => {
        if (
          event.data &&
          event.data.type === 'window-open' &&
          event.data.payload &&
          event.data.payload.url
        ) {
          window.open(event.data.payload.url, '_blank')
        }
      })
    </script>
    <script>
      const isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(
        navigator.userAgent
      )
      const autoWidth = window.location.search.includes('autowidth') && isMobile
      if (autoWidth) {
        const root = document.getElementById('root')
        if (root && root.classList) {
          root.classList.add('auto-width')
        }
      }
    </script>
